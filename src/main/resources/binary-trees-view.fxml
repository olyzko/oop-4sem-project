<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oopproject.controller.BinaryTreesController">
   <children>
      <Button layoutX="40.0" layoutY="14.0" mnemonicParsing="false" onAction="#onMainButton" text="main" />
      <TextField fx:id="nodesAdd" layoutX="324.0" layoutY="14.0" prefHeight="26.0" prefWidth="196.0" promptText="Enter nodes, separated by coma" />
      <TextField fx:id="nodesDelete" layoutX="324.0" layoutY="40.0" prefHeight="26.0" prefWidth="182.0" visible="false" />
      <Button fx:id="deleteButton" layoutX="506.0" layoutY="40.0" managed="false" mnemonicParsing="false" onAction="#DeleteButtonClicked" prefHeight="26.0" prefWidth="53.0" text="Delete" visible="false" />
      <Button fx:id="addButton" layoutX="520.0" layoutY="14.0" mnemonicParsing="false" onAction="#AddButtonClicked" text="Add" />
      <AnchorPane fx:id="treePane" layoutX="40.0" layoutY="67.0" prefHeight="299.0" prefWidth="520.0" />
      <Button fx:id="clearButton" layoutX="569.0" layoutY="442.0" mnemonicParsing="false" onAction="#ClearButtonClicked" text="Clear" />
      <ChoiceBox fx:id="choiceBox" layoutX="159.0" layoutY="14.0" prefWidth="150.0" />
      <Button fx:id="InorderButton" layoutX="54.0" layoutY="442.0" mnemonicParsing="false" onAction="#InorderButtonClicked" text="Inorder" />
      <Button fx:id="postorderButton" layoutX="141.0" layoutY="442.0" mnemonicParsing="false" onAction="#PostorderButtonClicked" text="Postorder" />
   </children>
</AnchorPane>
